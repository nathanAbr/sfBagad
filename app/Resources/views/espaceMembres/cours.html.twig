{% extends 'espaceMembres/layout.html.twig' %}

{% block content %}

    <!--{# for palmares in listPalmares #}-->
    <button type="button" class="btn btn-default btn-lg" data-toggle="modal" data-target=".bs-example-modal-lg">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
    </button>


    <div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Cours de batterie </h4>
                </div>
                <div class="modal-body">

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Fermer</button>
                    <button type="button" class="btn btn-primary"> Enregistrer</button>
                </div>
            </div>
        </div>
    </div>

    <div class="panel panel-default">
        <div class="panel-heading">
            <div class="input-group" >
                <h3>Sortie 1</h3>

                <div class="input-group-btn" style="padding-right: 350px">

                    <button type="button" class="btn btn-default">Supprimer</button>
                    <button type="button" class="btn btn-default" data-toggle="modal" data-target=".bs-example-modal-lg">Modifier</button>
                </div>
                <div class="input-group-btn">
                    <button type="button" class="btn btn-default">S'inscrire</button>
                    <button type="button" class="btn btn-default" data-toggle="modal" data-target=".bs-example-modal-lg">Détail de la sortie</button>

                </div>
            </div>
        </div>

        <div class="panel-body">
            <ul class="list-inline">
                <li> <strong>Date</strong></li>
                <li><strong>Heure de rdv</strong></li>
                <li><strong>Lieu de rdv</strong></li>
                <li><strong>Heure de retour</strong></li>
                <li></li>
            </ul>
        </div>
    </div>
    <!-- {# endfor #} -->

{% endblock %}

